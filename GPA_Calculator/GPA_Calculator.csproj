    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>qut-logo-og-1200.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="qut-logo-og-1200.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="qut-logo-og-1200.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>